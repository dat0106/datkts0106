<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<application>
		<default-render-kit-id>
			org.apache.myfaces.trinidad.core
		</default-render-kit-id>
	</application>
	<managed-bean>
		<managed-bean-name>tableValues</managed-bean-name>
		<managed-bean-class>de.vogella.jsf.trinidad.first.TableValues</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>myChartModel</managed-bean-name>
		<managed-bean-class>de.vogella.jsf.trinidad.first.MyChartModel</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>person</managed-bean-name>
		<managed-bean-class>de.vogella.jsf.trinidad.first.Person</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>


</faces-config>
