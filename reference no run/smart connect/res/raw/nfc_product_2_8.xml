<?xml version="1.0" encoding="utf-8"?>
<resources>
     <event default_enabled="1">
        <start_actions>
                <action id="com.sonyericsson.extras.liveware.actions.urllaunch.UrlLauncher"
                        setting="market://search?q=EA+Games"/>
                <action id="com.sonyericsson.extras.liveware.actions.soundmode.SoundMode"
                        setting= "normal"/>
                <action id="com.sonyericsson.extras.liveware.actions.volume.VolumeAction"
                        setting= "-1;80;-1"/>
                <action id="com.sonyericsson.extras.liveware.actions.facebook.FacebookAction"
                    setting= "5;@facebook_post_play"/>
        </start_actions>
        <stop_actions>
                <action id="com.sonyericsson.extras.liveware.actions.volume.VolumeAction"
                        setting= "-1;50;-1"/>
        </stop_actions>
    </event>
</resources>